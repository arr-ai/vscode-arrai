{
    "name": "vscode-arrai",
    "displayName": "arrai",
    "publisher": "arr-ai",
    "repository": {
        "type": "git",
        "url": "https://github.com/arr-ai/vscode-arrai.git"
    },
    "description": "VS Code Extension for the programming language Arrai",
    "version": "0.4.0",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:arrai"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "arrai",
                "aliases": [
                    "Arrai",
                    "arrai"
                ],
                "extensions": [
                    ".arrai"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "commands": [
            {
                "command": "extension.addConsoleLog",
                "title": "Add Console Log"
            }
        ],
        "grammars": [
            {
                "language": "arrai",
                "scopeName": "source.arrai",
                "path": "./syntaxes/arrai.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "tslint": "^6.1.2",
        "typescript": "^3.9.7",
        "vscode": "^1.1.37"
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    }
}
