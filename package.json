{
    "name": "vscode-arrai",
    "displayName": "arrai",
    "publisher": "arr-ai",
    "repository": {
        "type": "git",
        "url": "https://github.com/arr-ai/vscode-arrai.git"
    },
    "description": "VS Code Extension for the programming language Arrai",
    "version": "0.2.0",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "arrai",
                "aliases": [
                    "Arrai",
                    "arrai"
                ],
                "extensions": [
                    ".arrai"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "arrai",
                "scopeName": "source.arrai",
                "path": "./syntaxes/arrai.tmLanguage.json"
            }
        ]
    }
}
