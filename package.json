{
    "name": "vscode-arrai",
    "displayName": "vscode-arrai",
    "description": "VS Code Extension for the programming language Arrai",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "arrai",
                "aliases": [
                    "arrai",
                    "arrai"
                ],
                "extensions": [
                    ".arrai"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "arrai",
                "scopeName": "source.arrai",
                "path": "./syntaxes/arrai.tmLanguage.json"
            }
        ]
    }
}
